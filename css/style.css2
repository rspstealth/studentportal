/* Chooser button */
.chooser_button{
    margin:0px;
}
.chooser_button *{
    margin:0;
    padding:0;
}
.chooser_button ul li{
    background: #f5f5f5;
    margin-right: 5px;
    margin-bottom: 10px;
    border-radius: 6px;
    display: inline-block;
    font-weight: bold;
    min-width: 34px;
    text-align: center;
    cursor: pointer;
    padding: 5px 10px;
    -webkit-transition: all .2s;
    border: 2px solid transparent;
}
.toggle ul li:first-child{
}
.chooser_button ul li:first-child{
    border-radius: 6px;
    text-align: right;

}
.chooser_button ul li:last-child{
    border-radius: 6px;
    text-align: left;

}

.chooser_sm ul li span{
    min-width:100px !important;
}
.chooser_md ul li  span{
    min-width:150px !important;;
}
.chooser_lg ul li  span{
    min-width:220px !important;;
}
.chooser_button ul li:hover{
    color: white;
    background: #337ab7;
}
.chooser_button ul li.active{
    color: #337ab7;
    background: white;
    border: 2px solid #337ab7;
}

.manual_setup,.automatic_setup{
    opacity: 0;
    min-height: 25px;
    text-align: left;
    -webkit-transition:none;
}

.helper_menu{
    margin: 0;
    margin-bottom: 5px;
    padding: 0;
    height: 55px;
    border-bottom: 1px solid #f8f8f8;
}
.helper_menu .nav{
    margin: 12px 0px;
}

.helper_menu .nav>li>a{
    padding-right: 25px;
    margin: 0;
    color: #2a56c6;
    border-right: 1px solid #f5f5f5;
    border-radius: 0;
    background: transparent;
    height: 36px;
    line-height: 10px !important;
    text-align: left;
}
.helper_menu .nav>li>a:hover,.helper_menu .nav>li.active >a{
    color: #fff;
    background-color: #2a56c6;
    border-radius: 4px;
    border-bottom: 3px solid royalblue;
}
.breadcrumb_type{
    height: 50px;
    border-bottom: 1px solid #f2f2f2;
}
#basic_info_panel,#profile_info_panel,#profile_photo_panel{
    display:none;
}
.profile_panels{
    width:100%;
    float:left;
}
.app_notifications{
    margin-bottom: 0 !Important;
    -webkit-border-radius:0 !important;
    -moz-border-radius:0 !important;
    border-radius:0 !important;
    position: fixed;
    bottom:0;
    width:100%;
    height:50px;
    z-index: 99999;
    display: none;
}

#basic_info_panel.active,#profile_info_panel.active,#profile_photo_panel.active{
    display:block;
}
.breadcrumb_type .nav-pills>li >a{
    color: grey;
    background-color: transparent;
    padding-left: 20px;
    padding-right: 30px;
}
.breadcrumb_type .nav-pills>li.active a, .breadcrumb_type .nav-pills>li >a:hover,.breadcrumb_type .nav-pills>li >a:active,.breadcrumb_type .nav-pills>li >a:focus{
    color: #2a56c6;
    background-color: transparent;
}

.breadcrumb_type .nav-pills li:first-child a{
    padding-left: 0px;
}
.breadcrumb_type .nav-pills>li > span{
    right: 0;
