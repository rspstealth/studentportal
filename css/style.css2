.navbar-fixed-bottom, .navbar-fixed-top, .navbar-static-top{
    position: fixed;
    width: 100%;
    top: 0;
    min-height: 65px;
    background: #2a56c6;
}
.navbar-default .navbar-nav>li>a{
    color:white;
}
#wrapper{
    margin-top: 65px;
    padding: 0px;
    width: 100%;
    overflow: hidden;
}
.sidebar-menu {
    left: 0px;
    top: 65px;
    bottom: 50px;
    overflow-y: auto;
    position: fixed;
    /*    max-height: 600px;
        height: 600px;*/
    height: 598px;
    font: 400 15px Roboto,RobotoDraft,Helvetica,Arial,sans-serif;
    background-color: #ffffff;
    width: 19%;
    color: #616161;
    transition: all .4s ease 0s;
}

ul.main-menu {
    margin: 0px;
    padding: 0px;
    list-style-type: none;
    margin-top: 10px;
}

li.root-level {
    line-height: 45px;
}
.rotate{
    transform: rotate(180deg);
    transition: all .3s;
}
li.root-level .sub-menu{
    display:none;
    width:100% !important;
    -webkit-transition:none !important;
}
li.root-level a {
    color: #565656;
    display: block;
    text-decoration: none;
    padding: 0px 15px;
    cursor: pointer;
    -webkit-transition: .15s transform;
    transition: .15s transform;
    height: 44px;
    vertical-align: middle;
}

li.root-level a:hover{
    background:#dcdcdc;
    color:dodgerblue;
}
/*li.collapsed .down-arrow{*/
/*-webkit-transform:rotate(270deg);*/
/*}*/
/*li.down-arrow{*/
    /*-webkit-transform:rotate(0deg);*/
/*}*/

#wrapper .sidebar-menu #main-menu li a:hover {
    background-color: #f5f5f5;
    color: #616161;
}

li.root-level i {
    font-size: 20px;
}


li.root-level span {
    font:500 14px Roboto,RobotoDraft,Helvetica,Arial,sans-serif;
}

ul.sub-menu {
    padding: 0px;
    margin: 0px;
    list-style-type: none;
    border-bottom:1px solid #dcdcdc;
}

ul#main-menu li.active{
    background: #f8f8f8;
}
ul#main-menu li ul.sub-menu{
    background: #f8f8f8;
}
