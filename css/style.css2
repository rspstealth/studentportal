}
.package{
    width: 270px;
    margin: 11px;
    padding:10px;
    border-radius: 4px;
    border: 2px solid #f8f8f8;
}
.bronze,#bronze:hover{
    -webkit-transition:background .5s;
    background: #cd7f32;
    color: white;
    border-color: #b36c26;
    text-shadow:1px 1px 1px #874300;
}
.silver,#silver:hover{
    -webkit-transition:background .5s ease-out;
    color: white;
    text-shadow: 0px 1px 1px #7c7d7d;
    border-color: #acacac;
    background:silver;
}
.gold,#gold:hover{
    -webkit-transition:background .5s;
    background: #e4d750;
    color: white;
    border-color: #d3c63e;
    text-shadow: 1px 1px 1px #b5a60d;
}
.diamond,#diamond:hover{
    -webkit-transition:background .5s;
    background: #55d2ee;
    color: white;
    text-shadow: 0px 1px 1px #1688a2;
    border-color: #4cc5e0;
}
.bronze:hover{
    -webkit-transition:background .5s;
    background: #cd7f32;
    color: white;
    border-color: #b36c26;
    text-shadow:1px 1px 1px #874300;
}
.silver:hover{
    -webkit-transition: background .5s;
    background: #c3c3c3;
    color: white;
    text-shadow: 0px 1px 1px #7d7d7d;
    border-color: #acacac;
}
.gold:hover{
    -webkit-transition:background .5s;
    background: #e4d750;
    color: white;
    border-color: #d3c63e;
    text-shadow: 1px 1px 1px #b5a60d;
}
.platinum:hover{
    -webkit-transition:background .5s;
    background: #55d2ee;
    color: white;
    text-shadow: 0px 1px 1px #1688a2;
